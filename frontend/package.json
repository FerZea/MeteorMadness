  "dependencies": {
    "cesium": "^1.124.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "shx": "^0.3.4",
    "typescript": "^5.5.0",
    "vite": "^5.3.0"
  }
}
